$PROBLEM EXAMPLINIB1
$INPUT ID TIME EVID AMT CMT II ADDL RATE MDV DV S2_SAMPLING
$DATA data_to_fit004.csv IGNORE=@
$SUBROUTINES ADVAN13 TOL=8
$MODEL
COMP=(DEPOT) ; 1
COMP=(CENTRAL) ; 2

$PK 
iETA1 = 0
iETA2 = 0
iETA3 = 0
ETCL  = ETA(1) + iETA1 ; CL
ETVC  = ETA(2) + iETA2 ; VC
ETD2  = ETA(3) + iETA3 ; D2

TVCL   =  4.0
TVVC   =  70.0
TVD2   =  4.0

CL    = TVCL  * EXP(ETCL )
VC    = TVVC  * EXP(ETVC )
D2    = TVD2  * EXP(ETD2 )
K20 = CL / VC

S2 = VC ; dv in mg/l ; amt in mg

$DES
DADT(1) = 0
DADT(2) = - K20*A(2)
$OMEGA
0.2 ; CL
0.2 ; VC
0.2 ; D2
$SIGMA
0.05 FIX    ; prop
0 FIX ; additive

$ERROR
DEL=0
IF (F.EQ.0) DEL=1
W=F+DEL
Y=F+W*EPS(1)
IPRED=F
IRES=DV-IPRED
IWRES=IRES/W

$EST METHOD=1 INTER NOABORT MAXEVAL=0 FORMAT= s1PE16.8E3
$COV UNCONDITIONAL


$TABLE ID TIME EVID AMT CMT II ADDL RATE MDV DV S2_SAMPLING PRED IPRED NOPRINT NOAPPEND ONEHEADER FORMAT=s1PE16.8E3 FILE=run004.tab

