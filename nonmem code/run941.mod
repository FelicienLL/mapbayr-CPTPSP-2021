$PROBLEM Sunitinib, Yu et al, Br J Clin Pharmacol, 2014
$INPUT ID TIME EVID AMT CMT II ADDL MDV DV WT S2_SAMPLING
$DATA data_to_fit941.csv IGNORE=@
$SUBROUTINES ADVAN13 TOL=8
$MODEL COMP=(DEPOT) COMP=(CENTRALS, DEFOBS) COMP=(CENTRALM) COMP=(PERIPHM)

$PK 
iETA1 = 0
iETA2 = 0
iETA3 = 0
iETA4 = 0
ETVCS = ETA(1) + iETA1
ETVCM = ETA(2) + iETA2
ETCLM = ETA(3) + iETA3
ETCLS = ETA(4) + iETA4 

K12   = 0.34
TVCLS = 35.7
TVVCS = 1360
TVCLM = 17.1
TVVCM = 635 
TVQ   = 20.1
TVVPM = 388 

ASCL = (WT/70) ** 0.75
ASV  = WT/70
FM   = 0.21 
QH   = 80*ASCL 
CLS = TVCLS * ASCL * EXP(ETCLS) 
VCS = TVVCS * ASV  * EXP(ETVCS) 
CLM = TVCLM * ASCL * EXP(ETCLM) 
VCM = TVVCM * ASV  * EXP(ETVCM) 
VPM = TVVPM * ASV 
Q = TVQ * ASCL 
K34 = Q / VCM  
K43 = Q / VPM  

S2 = VCS/1000
S3 = VCM/1000

$DES
CLIV = ((K12 * A(1)) + (QH * A(2) / VCS)) / (QH + CLS)

DADT(1) = - K12 * A(1)
DADT(2) = QH * CLIV  - QH * A(2) / VCS
DADT(3) = FM * CLS * CLIV - CLM * A(3) / VCM - K34 * A(3) + K43 * A(4)
DADT(4) = K34 * A(3) - K43 * A(4)

$OMEGA BLOCK(4)
0.104976 ; VC suni
0.016892 0.335241 ; VC metab
0.000000 0.026738 0.177241 ; CL metab
0.000000 0.000000 0.010795 0.114921 ; CL suni

$SIGMA 
0.043 FIX 
0.023 FIX 

$ERROR
DEL=0
IF (F.EQ.0) DEL=1
W= F + DEL
Y = F + W * EPS(1)
IF(CMT.EQ.3) Y = F + W * EPS(2)
IPRED = F
IRES = DV - IPRED
IWRES = IRES / W

$EST METHOD=1 INTER NOABORT MAXEVAL=0 FORMAT= s1PE16.8E3
$COV UNCONDITIONAL


$TABLE ID TIME EVID AMT CMT II ADDL MDV DV WT S2_SAMPLING PRED IPRED NOPRINT NOAPPEND ONEHEADER FORMAT=s1PE16.8E3 FILE=run941.tab

