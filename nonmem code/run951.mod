$PROBLEM Methotrexate, Faltaos et al, CCP, 2006
$INPUT ID TIME EVID AMT CMT RATE MDV DV AGE SCR CYCLE S2_SAMPLING
$DATA data_to_fit951.csv IGNORE=@
$SUBROUTINES ADVAN13 TOL=8
$MODEL
COMP=(CENTRAL) ; 2
COMP=(PERIPH) ; 

$PK 
iETA1 = 0
iETA2 = 0
iETA3 = 0
iETA4 = 0
iETA5 = 0
iETA6 = 0
ETCL  = ETA(1) + iETA1 ; CL
ETVC  = ETA(2) + iETA2 ; VC
ETQ   = ETA(3) + iETA3 ; Q
ETVP  = ETA(4) + iETA4 ; VP
ETIOV1 = ETA(5) + iETA5 ; IOVCL 1
ETIOV2 = ETA(6) + iETA6 ; IOVCL 2

TVCL = 7.1 
TVVC = 25.1 
TVQ  = 0.15 
TVVP = 2.7 

AGE_CL = -0.22
SCR_CL = -0.43

IOVCL = ETIOV1
IF(CYCLE.NE.1) IOVCL = ETIOV2

CL = TVCL * EXP(ETCL + IOVCL) * ((SCR/67) ** SCR_CL) * ((AGE/62) ** AGE_CL)
VC = TVVC * EXP(ETVC)
Q  = TVQ  * EXP(ETQ)
VP = TVVP * EXP(ETVP)

K23 = Q / VC
K32 = Q / VP
K20 = CL / VC

S1 = VC / 2.2 ; dv in microM ; amt in mg

$DES
DADT(1) = - K20*A(1) - K23 * A(1) + K32 * A(2)
DADT(2) = K23 * A(1) - K32 * A(2)

$OMEGA BLOCK(2)
0.266256 ; CL
0.002401 0.233289 ; VC
$OMEGA BLOCK(2)
0.430336 ; Q
0.084164 0.4096  ; VP
$OMEGA
0.027225 ; IOV CL 1
0.027225 ; IOV CL 2

$SIGMA 
0.2116 ; err prop
225 ;  err additive

$ERROR
DEL=0
IF (F.EQ.0) DEL=1
W = F+DEL
Y = F +W*EPS(1) + EPS(2)
IPRED= F
IRES=DV-IPRED
IWRES=IRES/W

$EST METHOD=1 INTER NOABORT MAXEVAL=0 FORMAT= s1PE16.8E3
$COV UNCONDITIONAL


$TABLE ID TIME EVID AMT CMT RATE MDV DV AGE SCR CYCLE S2_SAMPLING PRED IPRED NOPRINT NOAPPEND ONEHEADER FORMAT=s1PE16.8E3 FILE=run951.tab

