$PROBLEM EXAMPLINIB1
$INPUT ID TIME EVID AMT CMT II ADDL RATE MDV DV S2_SAMPLING
$DATA data_to_fit402.csv IGNORE=@
$SUBROUTINES ADVAN13 TOL=8
$MODEL
COMP=(DEPOT) ; 1
COMP=(CENTRAL) ; 2
COMP=(CENTRALMET) ; 3

$PK 
iETA1 = 0
iETA2 = 0
iETA3 = 0
iETA4 = 0
iETA5 = 0
ETCL     = ETA(1) + iETA1 ; CL
ETVC     = ETA(2) + iETA2 ; VC
ETKA     = ETA(3) + iETA3 ; KA
ETCLmet  = ETA(4) + iETA4 ; CLmet
ETVCmet  = ETA(5) + iETA5 ; VCmet

TVCL   =  4.0
TVVC   =  70.0
TVKA   =  1.0
TVCLmet=  2.5
TVVCmet=  60.0

CL    = TVCL  * EXP(ETCL )
VC    = TVVC  * EXP(ETVC )
KA    = TVKA  * EXP(ETKA )
CLmet = TVCLmet  * EXP(ETCLmet )
VCmet = TVVCmet  * EXP(ETVCmet )

K23 = CL / VC
K30 = CLmet / VCmet

S2 = VC ; dv in mg/l ; amt in mg
S3 = VCmet
$DES
DADT(1) = -KA * A(1)
DADT(2) =  KA * A(1) - K23*A(2)
DADT(3) = .7*K23*A(2) - K30*A(3)

$OMEGA
0.2 ; CL
0.2 ; VC
0.2 ; KA 
0.2 ; CLmet
0.2 ; VCmet

$SIGMA
0.05 FIX    ; prop parent
0.05 FIX    ; prop met

$ERROR
DEL=0
IF (F.EQ.0) DEL=1
W=F+DEL
Y = F
IF(CMT.EQ.2) Y=F+W*EPS(1)
IF(CMT.EQ.3) Y=F+W*EPS(2)
IPRED=F
IRES=DV-IPRED
IWRES=IRES/W

$EST METHOD=1 INTER NOABORT MAXEVAL=0 FORMAT= s1PE16.8E3
$COV UNCONDITIONAL


$TABLE ID TIME EVID AMT CMT II ADDL RATE MDV DV S2_SAMPLING PRED IPRED NOPRINT NOAPPEND ONEHEADER FORMAT=s1PE16.8E3 FILE=run402.tab

