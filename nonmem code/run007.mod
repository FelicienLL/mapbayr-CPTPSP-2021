$PROBLEM EXAMPLINIB1
$INPUT ID TIME EVID AMT CMT II ADDL MDV DV S2_SAMPLING
$DATA data_to_fit007.csv IGNORE=@
$SUBROUTINES ADVAN13 TOL=8
$MODEL
COMP=(DEPOT1) ; 1
COMP=(CENTRAL) ; 2
COMP=(DEPOT3) ; 3

$PK 
iETA1 = 0
iETA2 = 0
iETA3 = 0
iETA4 = 0
ETCL  = ETA(1) + iETA1 ; CL
ETVC  = ETA(2) + iETA2 ; VC
ETKA1  = ETA(3) + iETA3 ; KA1
ETKA3  = ETA(4) + iETA4 ; KA3

TVCL   =  4.0
TVVC   =  70.0
TVKA1  =  1.0
TVKA3  =  .25

CL    = TVCL  * EXP(ETCL )
VC    = TVVC  * EXP(ETVC )
KA1   = TVKA1  * EXP(ETKA1 )
KA3   = TVKA3  * EXP(ETKA3 )

F1 = 0.2
F3 = 1 - F1

K20 = CL / VC

S2 = VC ; dv in mg/l ; amt in mg

$DES
DADT(1) = -KA1 * A(1)
DADT(2) =  KA1 * A(1) + KA3 * A(3) - K20*A(2)
DADT(3) = -KA3 * A(3)

$OMEGA
0.2 ; CL
0.2 ; VC
0.2 ; KA1 
0.2 ; KA3
$SIGMA
0.05 FIX    ; prop
0 FIX ; additive

$ERROR
DEL=0
IF (F.EQ.0) DEL=1
W=F+DEL
Y=F+W*EPS(1)
IPRED=F
IRES=DV-IPRED
IWRES=IRES/W

$EST METHOD=1 INTER NOABORT MAXEVAL=0 FORMAT= s1PE16.8E3
$COV UNCONDITIONAL


$TABLE ID TIME EVID AMT CMT II ADDL MDV DV S2_SAMPLING PRED IPRED NOPRINT NOAPPEND ONEHEADER FORMAT=s1PE16.8E3 FILE=run007.tab

